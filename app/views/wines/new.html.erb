<h1>Create a New Wine</h1>
<%= form_for(@wine) do |f| %>
  <%= f.label :Name %> <%= f.text_field :name, autofocus: true %><br />
  <%= f.label :Producer %> <%= f.text_field :winery %><br />
  <%= f.label :Year %> <%= f.text_field :year %><br />
  <%= f.label :Country %> <%= f.text_field :country %><br />
  <%= f.label :Varietal %> <%= f.text_field :varietal %><br />
  <p><%= f.submit %></p>
<% end %>